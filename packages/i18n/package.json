  "devDependencies": {
    "@nelsonlaidev/eslint-config": "2.4.5",
    "@nelsonlaidev/typescript-config": "1.1.1",
    "@types/react": "19.2.7",
    "next": "16.1.0",
    "next-intl": "4.6.1"
  },
  "peerDependencies": {
    "next": ">=16.0.0",
    "next-intl": ">=4.0.0"
  }
