  "dependencies": {
    "@ark-ui/react": "5.30.0",
    "@icons-pack/react-simple-icons": "13.8.0",
    "@repo/i18n": "workspace:*",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "cva": "1.0.0-beta.4",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "0.562.0",
    "merge-refs": "2.0.0",
    "next-themes": "0.4.6",
    "radix-ui": "1.4.3",
    "react-day-picker": "9.13.0",
    "react-resizable-panels": "4.0.12",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "vaul": "1.1.2"
