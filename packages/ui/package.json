  "devDependencies": {
    "@nelsonlaidev/eslint-config": "2.4.5",
    "@nelsonlaidev/typescript-config": "1.1.1",
    "@tailwindcss/typography": "0.5.19",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "next": "16.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0"
  },
  "peerDependencies": {
    "next": ">=16.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
